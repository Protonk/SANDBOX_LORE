(version 1)
(allow default)
;; Mimic bsd tail context with dtracehelper and posix_spawn literals plus a simple ioctl gate
(deny file-read* (literal "/dev/dtracehelper"))
(allow file-read* (literal "/usr/share/posix_spawn_filtering_rules"))
;; io-ctl not available in this SBPL; fall back to path-based allow/deny pairing
(deny file-write* (literal "/dev/dtracehelper"))
